# Google Cloud Platform Configuration
GCP_PROJECT_ID="your-project-id"
GCP_PROJECT_NUM="123456789012"
GCP_REGION="us-central1"
VERTEX_BUCKET="your-gcs-bucket"
GEMINI_API_KEY=""

# SQL Configuration
TABLE_NAME="your-table"
SQL_DATABASE_NAME="your-database"
SQL_USER="db-user"
SQL_PASSWORD="your-password"
SQL_INSTANCE_CONNECTION_NAME="project-id:region:instance-name"

# Concurrency Configuration
COOLDOWN_SECONDS="1.0"
MAX_RETRIES="8"
BATCH_TIMEOUT_HOURS="3"

# Local Postgres Configuration (Optional)
export USE_LOCAL_POSTGRES="false"
export LOCAL_POSTGRES_HOST="localhost"
export LOCAL_POSTGRES_PORT="5432"

# Round-robin Configuration
ROUND_ROBIN_ENABLED="false"

# Additional configuration for tests
TEST_PROJECT_ID="your-project-id"
TEST_REGION="us-central1"
TEST_BUCKET="your-gcs-bucket"